package Practice;
import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Sample {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner in=new Scanner(System.in);
        int t=in.nextInt();
        for(int j=1;j<=t;j++)
            {
        int a=in.nextInt();
        int b=in.nextInt();
        int n=in.nextInt();
        
        for(int i=0;i<n;i++)
            {
            a=a+(int)(Math.pow(2,i))*b;
            System.out.print(a+" ");
        }
            System.out.println();
        }
    }
}