package class9;

public class NullPointerExceptionScenario {
	public void mtd1()
	{
		System.out.println("mtd1");
	}

	public static void main(String[] args) {
		NullPointerExceptionScenario npes;
		try
		{
		npes.mtd1();
		}
		catch(Exception e)
		{
			e.printStackTrace();
		}
		// TODO Auto-generated method stub

	}

}
