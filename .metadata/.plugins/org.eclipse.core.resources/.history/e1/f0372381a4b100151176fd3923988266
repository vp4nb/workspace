package Class2;
class student
{
	int id=1;
	String name="raghav";
	String phone="112123312";
	public void display()
	{
		String str;
		System.out.println("Student details:\nid: "+id+"\nname: "+name+"\nphone: "+phone);
		str="hello";
	System.out.println("Student details:\nid: "+id+"\nname: "+name+"\nphone: "+phone+" str: ");
	}
}
public class Sample
	{
		public static void main(String[] args)
		{
			student std=new student();
			std.display();
		}
	}

