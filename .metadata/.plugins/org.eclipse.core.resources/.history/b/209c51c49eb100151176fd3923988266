package Class2;
class student
{
	int id=1;
	String name="raghav";
	String phone="112123312";
	public void display()
	{
	System.out.println("Student details:\n id: "+id+"\nname: "+name+"\nphone: "+phone);
	}
}
public class Sample
	{
		public static void main(String[] args)
		{
			student std=new student();
			std.display();
		}
	}

